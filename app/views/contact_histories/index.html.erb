<p id="notice"><%= notice %></p>

<h1>Contact Histories</h1>

<table>
  <thead>
    <tr>
      <th>Contact history</th>
      <th>Lookup</th>
      <th>Type</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_histories.each do |contact_history| %>
      <tr>
        <td><%= contact_history.contact_history_id %></td>
        <td><%= contact_history.lookup_id %></td>
        <td><%= contact_history.type %></td>
        <td><%= contact_history.date %></td>
        <td><%= link_to 'Show', contact_history %></td>
        <td><%= link_to 'Edit', edit_contact_history_path(contact_history) %></td>
        <td><%= link_to 'Destroy', contact_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact History', new_contact_history_path %>
